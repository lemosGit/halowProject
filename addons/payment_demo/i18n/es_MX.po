#. module: payment_demo
#: model_terms:ir.ui.view,arch_db:payment_demo.inline_form
msgid ""
"<select id=\"simulated_payment_state\" class=\"form-select\">\n"
"                        <option value=\"done\" title=\"Successful payment\">\n"
"                            Successful\n"
"                        </option>\n"
"                        <option value=\"pending\" title=\"Payment processing\">\n"
"                            Pending\n"
"                        </option>\n"
"                        <option value=\"cancel\" title=\"Payment canceled by customer\">\n"
"                            Canceled\n"
"                        </option>\n"
"                        <option value=\"error\" title=\"Processing error\">\n"
"                            Error\n"
"                        </option>\n"
"                    </select>"
msgstr ""
"<select id=\"simulated_payment_state\" class=\"form-select\">\n"
"                        <option value=\"done\" title=\"Pago exitoso\">\n"
"                            Exitoso\n"
"                        </option>\n"
"                        <option value=\"pending\" title=\"Procesando pago\">\n"
"                            Pendiente\n"
"                        </option>\n"
"                        <option value=\"cancel\" title=\"El cliente canceló el pago\">\n"
"                            Cancelado\n"
"                        </option>\n"
"                        <option value=\"error\" title=\"Error de procesamiento\">\n"
"                            Error\n"
"                        </option>\n"
"                    </select>"

